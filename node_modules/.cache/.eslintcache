ernode/Hyperswitch Backend to get the response.\n    // Please refer the link shared in the Readme file to setup merchant server\n    fetch(\"https://u4kkpaenwc.execute-api.ap-south-1.amazonaws.com/default/create-payment-intent\", {\n      method: \"POST\",\n      headers: { \"Content-Type\": \"application/json\" },\n      body: JSON.stringify(\n        { \n          amount: 20000, \n          currency: getCurrency(),\n          shipping:{\n            address:{\n              country:country,\n              state: \"test\",\n              zip: \"571201\",\n              line1: \"test line 1\",\n              city: \"test city\",\n              first_name:\"Bopanna MJ\",\n            },\n            phone: {\n              number:\"+918105528922\",\n              counrty_code:country\n            }\n          },\n          metadata: {\n            \"order_details\": {\n                \"product_name\": \"Apple iphone 15\",\n                \"quantity\": 1\n            }\n          },\n          billing: {\n            address: {\n              country: country,\n            },\n          },\n          authentication_type: 'no_three_ds', \n          customer_id: \"Bopanna12\"\n        }),\n    })\n      .then((res) => res.json())\n      .then((data) => {\n        // Set clientSecret from the response \n        data.clientSecret ? setClientSecret(data.clientSecret) : setClientSecret(\"pay_aJYDi517PIOMgLW7owcl_secret_9IEA0CX7ksFSnlO4Dj8r\")\n      })\n  }, [country]);\n\n  React.useEffect(() => {\n    // Get saved cards for a customer\n    // This fetch call is made to merchant server which inturn makes sever to sever call \n      // to Hypernode/Hyperswitch Backend to get the response.\n    // Please refer the link shared in the Readme file to setup merchant server\n    fetch(\"https://u4kkpaenwc.execute-api.ap-south-1.amazonaws.com/default/retrieve-customer\", {\n      method: \"POST\",\n      headers: { \"Content-Type\": \"application/json\" },\n      body: JSON.stringify(\n        { \n          customer_id: \"Bopanna12\"\n        }),\n    })\n      .then((res) => res.json())\n      .then((data) => {\n        //Set customer saved cards from the response \n        data.customer_payment_methods ? setSavedMethods(data.customer_payment_methods) : setSavedMethods([])\n      })\n  }, []);\n\n\n  return (\n    <div className=\"App\">\n      <Navbar countries={countries} setCountry={setCountry}/>\n      <Workspace clientSecret={clientSecret} savedMethods={savedMethods} />\n      <Footer />\n    </div>\n  );\n}\n\nexport default App;\n","/Users/prafulkoppalkar/Desktop/Repos/IntegrationDemoApp/src/reportWebVitals.js",[],[],"/Users/prafulkoppalkar/Desktop/Repos/IntegrationDemoApp/src/components/Workspace.jsx",["125","126","127","128"],[],"/Users/prafulkoppalkar/Desktop/Repos/IntegrationDemoApp/src/components/Footer.jsx",[],[],"/Users/prafulkoppalkar/Desktop/Repos/IntegrationDemoApp/src/components/Navbar.jsx",["129"],[],"/Users/prafulkoppalkar/Desktop/Repos/IntegrationDemoApp/src/components/Cart.jsx",["130"],[],"/Users/prafulkoppalkar/Desktop/Repos/IntegrationDemoApp/src/components/Confirm.jsx",["131","132","133","134"],[],"/Users/prafulkoppalkar/Desktop/Repos/IntegrationDemoApp/src/components/SDK.jsx",["135"],[],"/Users/prafulkoppalkar/Desktop/Repos/IntegrationDemoApp/src/components/CartItems.jsx",["136","137"],[],{"ruleId":"138","severity":1,"message":"139","line":65,"column":61,"nodeType":"140","messageId":"141","endLine":65,"endColumn":63},{"ruleId":"142","severity":1,"message":"143","line":112,"column":6,"nodeType":"144","endLine":112,"endColumn":15,"suggestions":"145"},{"ruleId":"146","severity":1,"message":"147","line":67,"column":13,"nodeType":"148","messageId":"141","endLine":67,"endColumn":25},{"ruleId":"142","severity":1,"message":"149","line":169,"column":8,"nodeType":"144","endLine":169,"endColumn":22,"suggestions":"150"},{"ruleId":"151","severity":1,"message":"152","line":3,"column":8,"nodeType":"153","messageId":"154","endL